{
  "name": "rehumanize",
  "short_name": "rehumanize",
  "version": "1.0.0",
  "manifest_version": 2,
  "author": "Agency",
  "description": "The Chrome extension for humanity.",
  "homepage_url": "http://rehumanize.me",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}